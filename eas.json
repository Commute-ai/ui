{
    "cli": {
        "version": ">= 16.4.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development",
            "environment": "development"
        },
        "staging": {
            "autoIncrement": true,
            "distribution": "internal",
            "channel": "staging",
            "environment": "preview",
            "android": {
                "buildType": "apk"
            }
        },
        "production": {
            "autoIncrement": true,
            "channel": "production",
            "environment": "production",
            "android": {
                "buildType": "apk"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
